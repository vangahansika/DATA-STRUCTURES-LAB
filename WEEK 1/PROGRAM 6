#include<iostream>
using namespace std;
int main()
{
int a,b;
char op;
cout<<"Enter a and b values";
cin>>a>>b;
cout<<"Enter operator";
cin>>op;
switch(op)
{
case'+':cout<<"addition:"<<a+b;
        break:
case'-':cout<<"substration:"<<a-b;
        break:
case'/':cout<<"division:"<<a/b;
        break:
case'%':cout<<"modulas:"<<a%b;
        break:
default:cout<<"invalid operator";
}
}
