#include<iostream>
using namesapce std;
int main()
{
int i,n,a[10],key,count=0;
cout<<"enter n and key value";
cin>>n>>key;
for(i=o;i<n;i++)
{
cout<<"Enter array elements"<<i+1<<":";
cin>>a[i];
if(a[i]==key)
{
count++;
}
}
if(count==00
cout<<key<<"not found";
