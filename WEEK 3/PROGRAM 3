#include<iostream>
using namespsce std;
int main()
{
int i,n,a[10],key;
bool status=flase;
cout<<"enter n and key values";
cin>>n>>key;
for(i=0;i<n,i++)
{
cout<<"enter element"<<i+1<<":";
cin>>a[i];
}
int low=0,high=n-1,mid;
while(low<=high)
{
mid=(low+high)/2;
if(a[mid]==key)
{
cout<<"key<<"found at locatin"<<mid;
status=true;
}
else if(a[mid]<key)
low=mid+1;
else
high=mid-1;
}
